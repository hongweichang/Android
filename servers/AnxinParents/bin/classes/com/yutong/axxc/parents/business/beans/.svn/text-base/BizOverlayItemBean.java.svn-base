package com.yutong.axxc.parents.business.beans;

import com.baidu.mapapi.OverlayItem;
import com.yutong.axxc.parents.common.GlobleConstants;
import com.yutong.axxc.parents.common.beans.LinePointInfoBean;

/**
 * 经纬度信息bean
 * 
 * @author <a href="mailto:wenxw@neusoft.com">sherly.wen </a>
 * @version $Revision 1.1 $ 2013-2-28 上午10:14:32
 */
public class BizOverlayItemBean
{
	private int marker;

	private OverlayItem overlayItem;

	private BizVehicleBean vehicleDetail;

	private String stationName = GlobleConstants.EMPTY_STR;// 站点名称

	private LinePointInfoBean locusItem;

	public LinePointInfoBean getLocusItem()
	{
		return locusItem;
	}

	public void setLocusItem(LinePointInfoBean locusItem)
	{
		this.locusItem = locusItem;
	}

	public String getStationName()
	{
		return stationName;
	}

	public void setStationName(String stationName)
	{
		this.stationName = stationName;
	}

	public BizVehicleBean getVehicleDetail()
	{
		return vehicleDetail;
	}

	public void setVehicleDetail(BizVehicleBean vehicleDetail)
	{
		this.vehicleDetail = vehicleDetail;
	}

	public BizOverlayItemBean(int defaultMark)
	{
		marker = defaultMark;
	}

	public int getMarker()
	{
		return marker;
	}

	public void setMarker(int marker)
	{
		this.marker = marker;
	}

	public OverlayItem getOverlayItem()
	{
		return overlayItem;
	}

	public void setOverlayItem(OverlayItem overlayItem)
	{
		this.overlayItem = overlayItem;
	}

}
