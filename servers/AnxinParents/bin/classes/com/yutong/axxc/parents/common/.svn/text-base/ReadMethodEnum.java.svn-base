package com.yutong.axxc.parents.common;

/**
 * 数据读取方式
 * @author zhangzhia 2013-9-5 上午11:53:44
 *
 */
public enum ReadMethodEnum
{
   /**
    * 本地获取
    */
    OPTYPE_LOCAL,
    /**
     * 网络获取
     */
    OPTYPE_REMOTE,
    /**
     * 先本地获取，如果本地无数据（数据为null或数据列表长度为0），再网络获取。
     */
    OPTYPE_LOCAL_AND_REMOTE,
    /**
     * 本地超时然后获取
     */
    CACHE_TIMEOUT_THEN_REMOTE
}
