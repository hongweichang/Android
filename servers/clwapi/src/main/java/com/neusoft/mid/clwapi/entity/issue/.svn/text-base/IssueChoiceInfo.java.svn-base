package com.neusoft.mid.clwapi.entity.issue;

import javax.xml.bind.annotation.XmlElement;

/**
 * @author <a href="mailto:majch@neusoft.com">majch </a>
 * @version $Revision 1.0 $ 2013-3-25 下午9:15:38
 */
public class IssueChoiceInfo {

	/**
	 * 备选答案编号.
	 */
	@XmlElement(name = "choice_id")
	private Long choiceId;

	/**
	 * 备选答案内容.
	 */
	@XmlElement(name = "choice_cont")
	private String choiceCont;

	/**
	 * @return Returns the choiceId.
	 */
	public Long getChoiceId() {
		return choiceId;
	}

	/**
	 * @param choiceId The choiceId to set.
	 */
	public void setChoiceId(Long choiceId) {
		this.choiceId = choiceId;
	}

	/**
	 * @return Returns the choiceCont.
	 */
	public String getChoiceCont() {
		return choiceCont;
	}

	/**
	 * @param choiceCont The choiceCont to set.
	 */
	public void setChoiceCont(String choiceCont) {
		this.choiceCont = choiceCont;
	}
}
