package com.neusoft.mid.clwapi.entity.news;

import javax.xml.bind.annotation.XmlElement;

/**
 * @author <a href="mailto:yi_liu@neusoft.com">yi_liu </a>
 * @version $Revision 1.0 $ 2013-5-6 下午4:43:28
 */
public class CheckNewsRequ {

	/**
	 * 消息ID
	 */
	@XmlElement(name = "news_id")
	private String newsId;

	/**
	 * 消息发布时间。格式：yyyymmddhh24miss
	 */
	@XmlElement(name = "news_time")
	private String newsTime;

	/**
	 * 消息类型0-行业政策1-宇通营销
	 */
	@XmlElement(name = "news_type")
	private String newsType;

	/**
	 * 用户ID
	 */
	private String usrId;

	/**
	 * @return Returns the usrId.
	 */
	public String getUsrId() {
		return usrId;
	}

	/**
	 * @param usrId
	 *            The usrId to set.
	 */
	public void setUsrId(String usrId) {
		this.usrId = usrId;
	}

	/**
	 * @return Returns the newsId.
	 */
	public String getNewsId() {
		return newsId;
	}

	/**
	 * @param newsId
	 *            The newsId to set.
	 */
	public void setNewsId(String newsId) {
		this.newsId = newsId;
	}

	/**
	 * @return Returns the newsTime.
	 */
	public String getNewsTime() {
		return newsTime;
	}

	/**
	 * @param newsTime
	 *            The newsTime to set.
	 */
	public void setNewsTime(String newsTime) {
		this.newsTime = newsTime;
	}

	/**
	 * @return Returns the newsType.
	 */
	public String getNewsType() {
		return newsType;
	}

	/**
	 * @param newsType
	 *            The newsType to set.
	 */
	public void setNewsType(String newsType) {
		this.newsType = newsType;
	}

}
