<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:axygb="http://schemas.android.com/apk/res/com.yutong.clw.ygbclient"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bg_maincolor" >

    <include
        android:id="@+id/title"
        layout="@layout/include_title" />

    <RelativeLayout
        android:id="@+id/rl_toggle"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_below="@+id/title"
        android:layout_marginLeft="30dp"
        android:layout_marginRight="30dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/bg_block" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="18dp"
            android:gravity="center"
            android:text="自动刷新"
            android:textSize="16sp" />

        <com.yutong.clw.ygbclient.view.widget.CommonCheckBox
            android:id="@+id/cb_map_refresh"
            android:layout_width="80dp"
            android:layout_height="36dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="20dp"
            android:shadowDx="2"
            android:shadowDy="2"
            android:textSize="16sp"
            android:textStyle="bold" />
    </RelativeLayout>

    <LinearLayout
        android:id="@+id/timeLL"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/rl_toggle"
        android:layout_marginLeft="30dp"
        android:layout_marginRight="30dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/bg_block"
        android:orientation="vertical" >

        <RelativeLayout
            android:id="@+id/fiveScondsRL"
            android:layout_width="fill_parent"
            android:layout_height="60dp"
            android:background="@drawable/bg_settingitem_topround" >

            <TextView
                android:id="@+id/tv_five_second"
                style="@style/text_setting_list_content"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="20dp"
                android:text="5秒钟"
                android:textAppearance="@style/content_font_16"
                android:textColor="@color/black" />

            <ImageView
                android:id="@+id/fiveScondsIV"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="25dp"
                android:src="@drawable/bg_listselected" />

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_alignParentBottom="true"
                android:background="@color/divider_color" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/tenScondsRL"
            android:layout_width="fill_parent"
            android:layout_height="60dp"
            android:background="@drawable/bg_settingitem" >

            <TextView
                android:id="@+id/tv_ten_second"
                style="@style/text_setting_list_content"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="20dp"
                android:text="10秒钟"
                android:textAppearance="@style/content_font_16"
                android:textColor="@color/black" />

            <ImageView
                android:id="@+id/tenScondsIV"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="25dp"
                android:src="@drawable/bg_listselected"
                android:visibility="invisible" />

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_alignParentBottom="true"
                android:background="@color/divider_color" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/fifteenScondsRL"
            android:layout_width="fill_parent"
            android:layout_height="60dp"
            android:background="@drawable/bg_settingitem_bottomround" >

            <TextView
                android:id="@+id/tv_fifteen_second"
                style="@style/text_setting_list_content"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="20dp"
                android:text="15秒钟"
                android:textAppearance="@style/content_font_16"
                android:textColor="@color/black" />

            <ImageView
                android:id="@+id/fifteenScondsIV"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="25dp"
                android:src="@drawable/bg_listselected"
                android:visibility="invisible" />

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_alignParentBottom="true"
                android:background="@color/divider_color" />
        </RelativeLayout>
    </LinearLayout>

</RelativeLayout>