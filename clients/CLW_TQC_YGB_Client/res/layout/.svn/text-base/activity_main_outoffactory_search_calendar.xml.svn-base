    android:layout_height="match_parent"
    android:background="#e8e8e8"
    android:orientation="vertical" >

    <include
        android:id="@+id/top_title"
        layout="@layout/include_title" />
	<RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
