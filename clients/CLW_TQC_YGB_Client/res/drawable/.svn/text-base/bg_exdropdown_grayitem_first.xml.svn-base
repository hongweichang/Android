<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
      <!-- first -->
    <item android:state_enabled="true" android:state_pressed="true"><shape>
            <corners android:bottomLeftRadius="0dp" android:bottomRightRadius="0dp" android:topLeftRadius="5dp" android:topRightRadius="5dp" />

            <solid android:color="#DADADA" />
        </shape></item>
    <item android:state_enabled="true"  android:state_pressed="false"><shape>
            <corners android:bottomLeftRadius="0dp" android:bottomRightRadius="0dp" android:topLeftRadius="5dp" android:topRightRadius="5dp" />

            <solid android:color="#E3E3E3" />
        </shape></item>
    <item android:state_enabled="false"><shape>
            <corners android:bottomLeftRadius="0dp" android:bottomRightRadius="0dp" android:topLeftRadius="5dp" android:topRightRadius="5dp" />

            <solid android:color="#999898" />
        </shape></item>

</selector>
