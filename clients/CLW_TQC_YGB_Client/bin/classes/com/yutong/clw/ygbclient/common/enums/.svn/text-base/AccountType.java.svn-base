package com.yutong.clw.ygbclient.common.enums;

/**
 * 账号类型
 * 
 * @author zhangzhia 2013-10-23 上午10:31:21
 * 
 */
public enum AccountType
{
	/**
	 * 所有类型,0
	 */
	AllType(0, "所有类型"),
	/**
	 * 员工号,1
	 */
	CodeType(1, "员工号"),
	/**
	 * 手机号码,2
	 */
	PhoneType(2, "手机号码");

	private int _value;

	private String _name;

	private AccountType(int value, String name)
	{
		_value = value;
		_name = name;
	}

	public int value()
	{
		return _value;
	}

	public String getName()
	{
		return _name;
	}

}
