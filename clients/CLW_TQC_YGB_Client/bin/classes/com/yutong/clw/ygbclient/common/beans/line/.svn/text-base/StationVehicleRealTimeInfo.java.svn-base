package com.yutong.clw.ygbclient.common.beans.line;

import java.util.ArrayList;
import java.util.List;

import com.yutong.clw.ygbclient.common.beans.StationInfo;
import com.yutong.clw.ygbclient.common.beans.VehicleRealtime;

/**
 * 站点与车辆相对实时数据
 * 
 * @author lizyi
 */
public class StationVehicleRealTimeInfo
{
    // 站点信息
    public StationInfo stationInfo;

    // 站点对应的车辆相对时时数据
    public List<VehicleRealtime> VehicleRealtimeInfos = new ArrayList<VehicleRealtime>();

    public StationInfo getStationInfos()
    {
        return stationInfo;
    }

    public void setStationInfos(StationInfo stationInfo)
    {
        this.stationInfo = stationInfo;
    }

    public List<VehicleRealtime> getVehicleRealtimeInfos()
    {
        return VehicleRealtimeInfos;
    }

    public void setVehicleRealtimeInfos(List<VehicleRealtime> vehicleRealtimeInfos)
    {
        VehicleRealtimeInfos = vehicleRealtimeInfos;
    }

    
}
