package com.yutong.clw.ygbclient.common.validation;

import java.util.ArrayList;
import java.util.List;

/**
 * 正则表达式对象
 * 
 * @author zhangzhia 2013-11-6 下午4:42:37
 */
public class VerifyResult {
	/**
	 * 有效性验证是否通过，默认为true
	 */
	public boolean isPass = true;

	/**
	 * 验证不通过描述
	 */
	public String faildesc;

	public List<VerifyResult> verifyResults = new ArrayList<VerifyResult>();

	public VerifyResult() {

	}

	public VerifyResult(boolean isPass, String faildesc) {
		this.isPass = isPass;
		this.faildesc = faildesc;

	}

}
